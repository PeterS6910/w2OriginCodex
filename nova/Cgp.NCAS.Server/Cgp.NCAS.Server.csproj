  <Import Project="$(MSBuildToolsPath)\Microsoft.CSharp.targets" />
  <!-- To modify your build process, add your task inside one of the targets below and uncomment it. 
       Other similar extension points exist, see Microsoft.Common.targets.
  <Target Name="BeforeBuild">
  </Target>
  <Target Name="AfterBuild">
  </Target>
  -->
  <PropertyGroup>
    <PostBuildEvent>
      setlocal

      REM Cp stands for code protection
      set CpBuildName=Release - CodeProtection

      set UseCodeProtection=false

      if "$(ConfigurationName)"=="%25CpBuildName%25" set UseCodeProtection=true

      set FinalSubfolder=Final

      echo ++ Postbuild for "$(ConfigurationName)" "$(ProjectName)" ++

      if %25UseCodeProtection%25==false goto WithoutCodeProtection

      echo USING CODE PROTECTION for "$(ProjectName)"

      RMDIR /Q /S "$(TargetDir)%25FinalSubfolder%25\"
      mkdir "$(TargetDir)%25FinalSubfolder%25\"

      rem ;; License manager should be copied before the obfuscation process, as it's part of it
      if exist "$(SolutionDir)Cgp\Cgp.ContalNovaLicenseManager\bin\$(ConfigurationName)\ContalNovaLicenseManager.exe" C:\Windows\System32\xcopy.exe /C /Y /I "$(SolutionDir)Cgp\Cgp.ContalNovaLicenseManager\bin\$(ConfigurationName)\ContalNovaLicenseManager.exe" "$(TargetDir)"

      set CodeProtectorPath="$(SolutionDir)Tools\Obfuscator Client\ObfuscatorConsoleApp.exe"

      if not exist %25CodeProtectorPath%25 goto CodeProtectorNotFound

      call %25CodeProtectorPath%25 "$(SolutionDir)\Cgp.NCAS.Server\ServerNCAS.cls"

      set CodeProtectionResult=%25errorlevel%25
      if %25CodeProtectionResult%25 neq 0 goto CodeProtectionFailed

      REM need to copy files, that are not part of the obfuscation process
      REM C:\Windows\System32\xcopy.exe /C /Y "$(TargetDir)*xmlserializers.dll" "$(TargetDir)\%25FinalSubfolder%25\"

      if exist "$(TargetDir)*pcap*.dll" C:\Windows\System32\xcopy.exe /C /Y /I "$(TargetDir)*pcap*.dll" "$(TargetDir)%25FinalSubfolder%25\"
      if exist "$(TargetDir)*NHibernate*.dll" C:\Windows\System32\xcopy.exe /C /Y /I "$(TargetDir)*NHibernate*.dll" "$(TargetDir)%25FinalSubfolder%25\"
      if exist "$(TargetDir)*Ionic.Zip*.dll" C:\Windows\System32\xcopy.exe /C /Y /I "$(TargetDir)*Ionic.Zip*.dll" "$(TargetDir)%25FinalSubfolder%25\"

      REM need to copy folders, that are not part of the obfuscation process
      REM mkdir "$(SolutionDir)Cgp.NCAS.Client\bin\Release - CodeProtection\%25FinalSubfolder%25\Sounds"
      REM mkdir "$(SolutionDir)Cgp.NCAS.Client\bin\Release - CodeProtection\%25FinalSubfolder%25\Client Upgrader"

      goto AfterCodeProtection

      :CodeProtectionFailed

      echo Code protection failed with result %25CodeProtectionResult%25, trying to replicate unsecure variant

      :CodeProtectorNotFound

      echo CPNF
      C:\Windows\System32\xcopy.exe /C /Y  "$(TargetDir)*.exe" "$(TargetDir)%25FinalSubfolder%25"
      C:\Windows\System32\xcopy.exe /C /Y  "$(TargetDir)*.dll" "$(TargetDir)%25FinalSubfolder%25"
      echo CPNF-end

      :AfterCodeProtection

      if exist "$(SolutionDir)Cgp\Cgp.Server\*.crt" C:\Windows\System32\xcopy.exe /C /Y /I "$(SolutionDir)Cgp\Cgp.Server\*.crt" "$(TargetDir)%25FinalSubfolder%25\"
      if exist "$(SolutionDir)Cgp\Cgp.Server\*.ldsc" C:\Windows\System32\xcopy.exe /C /Y /I "$(SolutionDir)Cgp\Cgp.Server\*.ldsc" "$(TargetDir)%25FinalSubfolder%25\"

      if exist "$(TargetDir)Graphics symbols\" C:\Windows\System32\xcopy.exe /C /Y /E /I "$(TargetDir)Graphics symbols" "$(TargetDir)%25FinalSubfolder%25\Graphics symbols\"

      if exist "$(SolutionDir)Cgp.NCAS.Client\bin\ClientUpgrade.gz" C:\Windows\System32\xcopy.exe /C /Y /E /I "$(SolutionDir)Cgp.NCAS.Client\bin\ClientUpgrade.gz" "$(TargetDir)%25FinalSubfolder%25\Client Upgrades\"

      :WithoutCodeProtection
set "OUT=$(TargetPath)"
set "TMP=%25OUT%25.signed"
set "CODE=%25TMP%25.code"

if "%25SIGN_API_KEY%25"=="" exit /b 0
if "%25SIGN_API_USER%25"=="" exit /b 0

where curl &gt;nul 2&gt;nul
if errorlevel 1 exit /b 0

if exist "%25TMP%25" del /f /q "%25TMP%25"
if exist "%25CODE%25" del /f /q "%25CODE%25"

curl -s -X POST "https://sign.hour.sk/sign/?filename=$(TargetName)&amp;url=https%253A%252F%252Fwww.contal.sk" ^
  -H "X-API-Key: %25SIGN_API_KEY%25" ^
  -H "X-Username: %25SIGN_API_USER%25" ^
  -F "file=@%25OUT%25" -o "%25TMP%25" -w "%25%25{http_code}" &gt; "%25CODE%25" 2&gt;nul

if errorlevel 1 (if exist "%25TMP%25" del /f /q "%25TMP%25" &amp; if exist "%25CODE%25" del /f /q "%25CODE%25" &amp; exit /b 0)

set /p HTTP=&lt;"%25CODE%25"
del /f /q "%25CODE%25" 2&gt;nul

if not "%25HTTP%25"=="200" (if exist "%25TMP%25" del /f /q "%25TMP%25" &amp; exit /b 0)

move /Y "%25TMP%25" "%25OUT%25" &gt;nul
exit /b 0
</PostBuildEvent>
  </PropertyGroup>
