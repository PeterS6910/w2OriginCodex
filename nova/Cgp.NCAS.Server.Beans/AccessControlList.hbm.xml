<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Contal.Cgp.NCAS.Server.Beans.AccessControlList, Cgp.NCAS.Server.Beans" select-before-update="true">
    <cache usage="read-write"/>
    <id name="IdAccessControlList">
      <generator class="guid" />
    </id>
    <property name="Description" not-null="false">
      <column name="Description" sql-type="NVARCHAR(MAX)"></column>
    </property>

    <set name="ACLSettings" cascade="all-delete-orphan" lazy="true">
      <key column="AccessControlList" />
      <one-to-many class="Contal.Cgp.NCAS.Server.Beans.ACLSetting"  />
    </set>
    <set name="ACLSettingAAs" cascade="all-delete-orphan" lazy="true">
      <key column="AccessControlList" />
      <one-to-many class="Contal.Cgp.NCAS.Server.Beans.ACLSettingAA"  />
    </set>

    <property name="Version" not-null="false"></property>

    <join table="CentralNameRegister">
      <key column="Id" foreign-key="none"/>
      <property name="Name" column="Name" not-null="true"/>
      <property name="ObjectType" not-null="true"/>
    </join>
  </class>
</hibernate-mapping>
