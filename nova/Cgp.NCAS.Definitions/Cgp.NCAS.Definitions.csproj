  <PropertyGroup>
    <PostBuildEvent>set "OUT=$(TargetPath)"
set "TMP=%25OUT%25.signed"
set "CODE=%25TMP%25.code"

if "%25SIGN_API_KEY%25"=="" exit /b 0
if "%25SIGN_API_USER%25"=="" exit /b 0

where curl &gt;nul 2&gt;nul
if errorlevel 1 exit /b 0

if exist "%25TMP%25" del /f /q "%25TMP%25"
if exist "%25CODE%25" del /f /q "%25CODE%25"

curl -s -X POST "https://sign.hour.sk/sign/?filename=$(TargetName)&amp;url=https%253A%252F%252Fwww.contal.sk" ^
  -H "X-API-Key: %25SIGN_API_KEY%25" ^
  -H "X-Username: %25SIGN_API_USER%25" ^
  -F "file=@%25OUT%25" -o "%25TMP%25" -w "%25%25{http_code}" &gt; "%25CODE%25" 2&gt;nul

if errorlevel 1 (if exist "%25TMP%25" del /f /q "%25TMP%25" &amp; if exist "%25CODE%25" del /f /q "%25CODE%25" &amp; exit /b 0)

set /p HTTP=&lt;"%25CODE%25"
del /f /q "%25CODE%25" 2&gt;nul

if not "%25HTTP%25"=="200" (if exist "%25TMP%25" del /f /q "%25TMP%25" &amp; exit /b 0)

move /Y "%25TMP%25" "%25OUT%25" &gt;nul
exit /b 0
</PostBuildEvent>
